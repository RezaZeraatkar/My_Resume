{
  "name": "resume-api",
  "version": "1.0.0",
  "description": "\"this is to serve my resume react app\"",
  "main": "index.js",
  "author": "Reza Zeraatkar <r.zeraatkar1992@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.3.1",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "deploy": "yarn install && yarn install --prefix client && yarn run validate --prefix client",
    "heroku-postbuild": "yarn install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "mongoose": "^5.6.1",
    "morgan": "^1.9.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "concurrently": "4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "^2.3.0",
    "nodemon": "1.19.1",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RezaZeraatkar/My_Resume"
  },
  "bugs": "https://github.com/RezaZeraatkar/My_Resume/issues"
}
